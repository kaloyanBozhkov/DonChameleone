{
  "name": "don-chameleone-server",
  "scripts": {
    "build": "tsc ./src/env.ts && node ./src/env.js && webpack",
    "dev": "tsc ./src/env.ts && node ./src/env.js && tsx --watch ./src/index.ts",
    "eslint_check": "eslint './src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx}'",
    "type_watch": "tsc --project . --watch",
    "type_check": "tsc --project .",
    "prettier_check": "prettier --check 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json,*.scss.d.ts,.scss.d.ts}'",
    "check_all": "npm run prettier_check && npm run eslint_check && npm run type_check",
    "eslint_fix": "eslint --fix",
    "prettier_fix": "prettier --write 'src/**/*.{js,*.js,ts,*.ts,tsx,*.tsx,scss,*.scss,css,*.css,json,*.scss.d.ts,.scss.d.ts}'",
    "fix_all": "npm run prettier_fix && npm run eslint_fix",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "start": "node ../dist/server/bundle.js"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@trpc/server": "^10.44.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "prisma": "^5.7.1",
    "serverless-http": "^3.2.0",
    "superjson": "^2.2.1",
    "trpc-panel": "^1.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "copy-node-modules": "^1.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "serverless-webpack": "^5.13.0",
    "ts-loader": "^9.5.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
